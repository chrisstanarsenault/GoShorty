<h1>Shorten R Us</h1>
<% if (user_id) { %>
  <p>Oh, hey there <%= user_id.username %>!</p>
  <form method="POST" action="/logout">
    <input type="submit" value="LOGOUT" />
  </form>
<% } else { %>
  <p><a href="/login">Login </a> | <a href="/register">Register </a></p>
<% } %>
